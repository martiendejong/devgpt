<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <LanguageVersion>latest</LanguageVersion>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>DevGPT.Store</RootNamespace>
    <Version>1.1.3</Version>
    <PackageVersion>1.1.3</PackageVersion>
    <Authors>Prospergenics</Authors>
    <Description>Document storage and retrieval system for RAG (Retrieval-Augmented Generation) in DevGPT. Provides IDocumentStore interface with support for text/binary documents, chunking, metadata management, and relevancy matching. Includes file-based and memory-based backends.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/prospergenics/devgpt</PackageProjectUrl>
    <RepositoryUrl>https://github.com/prospergenics/devgpt</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>rag;document-store;retrieval;storage;chunking;embeddings;semantic-search</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\EmbeddingStore\DevGPT.Store.EmbeddingStore.csproj" />
    <ProjectReference Include="..\..\LLMs\Helpers\DevGPT.LLMs.Helpers.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Npgsql" Version="8.0.3" />
  </ItemGroup>
</Project>