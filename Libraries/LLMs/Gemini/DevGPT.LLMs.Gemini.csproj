<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <LanguageVersion>latest</LanguageVersion>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>DevGPT.LLMs</RootNamespace>
    <Version>1.1.3</Version>
    <PackageVersion>1.1.3</PackageVersion>
    <Authors>Prospergenics</Authors>
    <Description>Google Gemini implementation of ILLMClient for DevGPT. Provides access to Gemini models with chat completions, streaming, system instructions, structured JSON outputs, and token usage tracking. Supports Gemini Pro and other Google AI models.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/prospergenics/devgpt</PackageProjectUrl>
    <RepositoryUrl>https://github.com/prospergenics/devgpt</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>google;gemini;llm;ai;chat;streaming;gemini-pro;google-ai</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\Client\DevGPT.LLMs.Client.csproj" />
    <ProjectReference Include="..\Classes\DevGPT.LLMs.Classes.csproj" />
    <ProjectReference Include="..\Helpers\DevGPT.LLMs.Helpers.csproj" />
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="9.0.4" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Binder" Version="9.0.4" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="9.0.4" />
  </ItemGroup>
</Project>