<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <LanguageVersion>latest</LanguageVersion>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>DevGPT.LLMs</RootNamespace>
    <Version>1.1.4</Version>
    <PackageVersion>1.1.4</PackageVersion>
    <Authors>Prospergenics</Authors>
    <Description>Core data models and contracts for the DevGPT ecosystem. Provides chat message models, LLM response wrappers with token usage tracking, tool definitions, image generation models, and shared interfaces used across all DevGPT packages.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/prospergenics/devgpt</PackageProjectUrl>
    <RepositoryUrl>https://github.com/prospergenics/devgpt</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>llm;ai;language-model;chat;openai;anthropic;claude;gpt;tokens;tool-calling</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="System.Memory.Data" Version="6.0.0" />
  </ItemGroup>
</Project>